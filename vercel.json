{
    "regions": ["sin1"],
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/cron.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/cron",
            "dest": "api/cron.py"
        },
        {
            "src": "/webhook/(.*)",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 9 * * *"
        }
    ],
    "env": {
        "ENVIRONMENT": "production"
    }
}